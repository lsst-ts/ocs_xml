<ocs:ocsCommandSequence xmlns:ocs="http://project.lsst.org/ts" Date="2016-02-09T08:40:51.678" OnError="Continue">

 <!-- one way of doing it -->
 <ocs:genericCommandSequence>

  <!-- disable command test -->
  <ocs:disable Subsystem="AuxiliaryMount" WaitForCompletion="false"/>
  <ocs:disable Subsystem="Camera" WaitForCompletion="false"/>
  <ocs:disable Subsystem="LSSTMount" WaitForCompletion="false"/>
  <!-- synchronization point: TBD -->

  <!-- standby command test -->
  <ocs:standby Subsystem="AuxiliaryMount" WaitForCompletion="false"/>
  <ocs:standby Subsystem="Camera" WaitForCompletion="false"/>
  <ocs:standby Subsystem="LSSTMount" WaitForCompletion="false"/>
  <!-- synchronization point: TBD -->

  <!-- exit control command test -->
  <ocs:exitcontrol Subsystem="AuxiliaryMount" WaitForCompletion="false"/>
  <ocs:exitcontrol Subsystem="Camera" WaitForCompletion="false"/>
  <ocs:exitcontrol Subsystem="LSSTMount" WaitForCompletion="false"/>

 </ocs:genericCommandSequence>

